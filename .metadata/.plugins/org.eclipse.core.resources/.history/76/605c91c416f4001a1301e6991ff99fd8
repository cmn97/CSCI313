public class NodePoly<T> extends Node {
	private int coeff, power;
	private Node next;
	
	NodePoly(int x, int y, Node term) {
		coeff = x;
		power = y;
		next = term;
	}
	
	public int getCoeff() {
		return coeff;
	}
	
	public int getPower() {
		return power;
	}
	
